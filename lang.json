{
    "Lexer": 
    {
        "token-defs": 
        {
            "whitespace": [" ", "\n", "\t"],
            "digit": [],
            "letter": []
        },
        "token-syntax":
        {
            "+": "ADD",
            "-": "SUB",
            "*": "MUL",
            "/": "DIV",
            "%": "MOD",
            "(": "CLBRAC",
            ")": "CRBRAC",
            "[": "SLBRAC",
            "]": "SRBRAC",
            "{": "FLBRAC",
            "}": "FRBRAC",
            "<": "ALBRAC",
            ">": "ARBRAC",
            "=": "EQUAL",
            ",": "COMMA",
            ";": "SEMICOLON",
            "|": "LINE",
            "\"": "QUOTE",
            "!": "EXCLAMATION",
            "^": "EXPO",
            "&": "AND",
            "~": "OR",
            "@": "AT"
        },
        "token-id":
            [
                ">>",
                "abs",
                "max",
                "min",
                "int",
                "float",
                "exp",
                "set",
                "iszero",
                "cell",
                "privcell",
                "strnum",
                "numstr",
                "concat",
                "strlen",
                "func",
                "app",
                "ele",
                "push",
                "append",
                "head",
                "tail",
                "arrlen",
                "if",
                "then",
                "else",
                "while",
                "import",
                "print",
                "tostring",
                "pycell"
            ]
    }
}