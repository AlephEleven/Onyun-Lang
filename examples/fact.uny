>> Calculates factorial of a

cell
(  
    set numbool = func(a, !iszero(a)),
    set fact = func(a,
        (
            set res = 1,
            while([app(numbool, a)],
                (
                set res = res*a,
                set a = a-1
                ) |> cell
            ),
            res
        ) |> cell
    ),

    set a = 5,
    app(fact, a)
) 